<> rdfs:label "testing monster multi" .

:Attack a rdfs:Class.

:alice a :Monster;
    :name "alice";
    :attack ( [ a :Attack; rdf:value ("hax" 10) ] );
.

:bob a :Monster;
    :name "bob";
    :attack ( [ a :Attack; rdf:value ("bite" 20) ] 
              [ a :Attack; rdf:value ("claw" 21) ]
              [ a :Attack; rdf:value ("wang" 22) ]);
.

